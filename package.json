{
    "name": "education-portal",
    "version": "1.0.0",
    "description": "Redesign for the mijnhva education portal",
    "main": "server.js",
    "scripts": {
        "prestart": "link-module-alias && npm run build",
        "start": "node server.js",
        "prebuild": "rimraf public/css/* && rimraf public/js/*",
        "build": "gulp",
        "watch": "gulp watch",
        "predev": "npm run build",
        "dev": "nodemon server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WesselSmit/education-portal.git"
    },
    "author": "Wessel Smit & Sjors Eveleens",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/WesselSmit/education-portal/issues"
    },
    "homepage": "https://github.com/WesselSmit/education-portal#readme",
    "dependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "babelify": "^10.0.0",
        "body-parser": "^1.19.0",
        "browserify": "^16.5.1",
        "dotenv": "^8.2.0",
        "ejs": "^3.0.1",
        "express": "^4.17.1",
        "express-ejs-layouts": "^2.5.0",
        "gulp": "^4.0.2",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-sass": "^4.0.2",
        "gulp-sass-glob": "^1.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "link-module-alias": "^1.2.0",
        "mongodb": "^3.5.7",
        "require-dir": "^1.2.0",
        "rimraf": "^3.0.2",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.2"
    },
    "_moduleAliases": {
        "#builds": "build-scripts",
        "#src": "src",
        "#public": "public",
        "#views": "views",
        "#routes": "routes",
        "#data": "data"
    }
}
